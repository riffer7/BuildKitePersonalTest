		<ee:transform doc:name="Print is alive" doc:id="1d7b19f2-c132-49bf-96ac-6008631ffe51" >
			<ee:message >
				<ee:set-payload ><![CDATA[%dw 2.0
output application/json
---
{
	"isAlive" : true
}]]></ee:set-payload>
			</ee:message>
		</ee:transform>
		<logger level="INFO" doc:name="Logger" doc:id="566e44b0-27f8-4f70-9085-6d6864ca3380" />
